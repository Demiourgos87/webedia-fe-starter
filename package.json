{
  "name": "webpack-yarn",
  "version": "0.0.1",
  "description": "Webpack FrontEnd starter with yarn",
  "main": "index.js",
  "scripts": {
    "dev-live": "webpack --watch --progress --config=dev.config.js",
    "dev-local": "webpack-dev-server --hot --progress --colors --config=dev.config.js",
    "build": "webpack --progress --config=build.config.js",
    "prod": "webpack -p --progress --config=build.config.js"
  },
  "author": "Bojan Knezevic",
  "license": "MIT",
  "dependencies": {
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-hot-loader": "^1.3.4",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "jquery": "^3.2.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack-build-notifier": "^0.1.21",
    "webpack-dev-server": "^2.9.7"
  }
}
